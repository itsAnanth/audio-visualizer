{
    "compilerOptions": {
        "strictNullChecks": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "target": "ESNext",
        "module": "CommonJS",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "strict": true,
        "jsx": "react",
        "outDir": "out",
        "lib": ["DOM"],
        "sourceMap": false
    },
    "exclude": ["**/out/"],
}